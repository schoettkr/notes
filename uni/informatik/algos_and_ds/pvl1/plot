set style line 1 \
  linecolor rgb '#0060ad' \
  linetype 1 linewidth 2 \
  pointtype 7 pointsize 1
set logscale y
set logscale x 2
set xlabel "N"
set ylabel "Anzahl"
plot 'P.dat' with linespoints linestyle 1 lt rgb "violet" title "Primzahlen",\
"S.dat" with linespoints linestyle 1 title "Streichungen"


